{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "dailyTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 10 * * *",
      "useMonitor": false
    },
    {
      "name": "prsIn",
      "type": "blob",
      "dataType": "string",
      "direction": "in",
      "path": "data/pullrequests.jsonl",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "projectsIn",
      "type": "blob",
      "direction": "in",
      "connection": "AzureWebJobsStorage",
      "dataType": "string",
      "path": "data/projects.jsonl"
    },
    {
      "name": "contributorsIn",
      "type": "blob",
      "direction": "in",
      "connection": "AzureWebJobsStorage",
      "dataType": "string",
      "path": "data/contributors.jsonl"
    }
  ]
}