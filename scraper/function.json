{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "scraperTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 9 * * *",
      "useMonitor": false
    },
    {
      "name": "prsIn",
      "type": "blob",
      "direction": "in",
      "connection": "AzureWebJobsStorage",
      "dataType": "string",
      "path": "data/pullrequests.jsonl"
    },
    {
      "name": "projectsIn",
      "type": "blob",
      "direction": "in",
      "connection": "AzureWebJobsStorage",
      "dataType": "string",
      "path": "data/projects.jsonl"
    },
    {
      "name": "contributorsIn",
      "type": "blob",
      "direction": "in",
      "connection": "AzureWebJobsStorage",
      "dataType": "string",
      "path": "data/contributors.jsonl"
    },
    {
      "name": "prsOut",
      "type": "blob",
      "direction": "out",
      "connection": "AzureWebJobsStorage",
      "dataType": "string",
      "path": "data/pullrequests.jsonl"
    },
    {
      "name": "projectsOut",
      "type": "blob",
      "direction": "out",
      "connection": "AzureWebJobsStorage",
      "dataType": "string",
      "path": "data/projects.jsonl"
    },
    {
      "name": "contributorsOut",
      "type": "blob",
      "direction": "out",
      "connection": "AzureWebJobsStorage",
      "dataType": "string",
      "path": "data/contributors.jsonl"
    }
  ]
}